@model List<Employee>

<h1>Employee List</h1>
<ol>
   
    @foreach(var employee in Model)
    {
        
        <li>@employee.Name
            <ul>
             <li>Department: @employee.Department</li>
            <li>Phone Number: @employee.PhoneNumber</li>
            <li>Email: @employee.Email</li>
            <li>Address: @employee.Address</li>
        </ul>
        </li>
        
    }
   
</ol>